{
  "name": "setbased/abc-babel-core",
  "description": "ABC Framework: Core Babel",
  "keywords": [
    "ABC-Framework",
    "babel",
    "core"
  ],
  "require": {
    "php": ">=7.0.0",
    "setbased/abc": "^0.10.2",
    "setbased/abc-babel": "^0.10.0",
    "setbased/abc-html": "^1.3.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^6.0.0"
  },
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\Babel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Babel\\Test\\": "test"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
