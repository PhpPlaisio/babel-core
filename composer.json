{
  "name": "plaisio/babel-core",
  "description": "PhpPlaisio: Core Babel",
  "keywords": [
    "PhpPlaisio",
    "Babel",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/babel": "^2.0.1",
    "plaisio/helper-html": "^1.1",
    "plaisio/kernel": "^3.0",
    "setbased/exception": "^2.1"
  },
  "require-dev": {
    "ext-mysqli": "*",
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "plaisio/console": "^2.0.1",
    "setbased/php-stratum": "^6.0",
    "setbased/php-stratum-mysql": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Babel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Babel\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
