{
  "name": "plaisio/babel-core",
  "description": "PhpPlaisio: Core Babel",
  "keywords": [
    "PhpPlaisio",
    "Babel",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-intl": "*",
    "plaisio/babel": "^2.1.0",
    "plaisio/helper-html": "^4.1.0",
    "plaisio/kernel": "^3.2.0",
    "setbased/exception": "^2.3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/console-kernel": "^1.0.0",
    "setbased/error-handler": "^1.3.0",
    "setbased/php-stratum": "^6.3.1",
    "setbased/php-stratum-mysql": "^6.6.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Babel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Babel\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
