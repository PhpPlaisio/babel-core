{
  "name": "plaisio/babel-core",
  "description": "PhpPlaisio: Core Babel",
  "keywords": [
    "PhpPlaisio",
    "Babel",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-intl": "*",
    "plaisio/babel": "^2.2.0",
    "plaisio/helper-html": "^4.2.0",
    "plaisio/kernel": "^3.3.0",
    "setbased/exception": "^2.6.0",
    "setbased/php-stratum-mysql": "^7.1.1"
  },
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/console-kernel": "^1.2.0",
    "setbased/error-handler": "^1.4.0",
    "setbased/php-stratum": "^6.5.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Babel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Babel\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
