{
  "name": "setbased/abc-babel-core",
  "description": "ABC-Framework: Core Babel",
  "keywords": [
    "ABC-Framework",
    "Babel",
    "Core"
  ],
  "require": {
    "php": ">=7.1.0",
    "setbased/abc": "*",
    "setbased/abc-babel": "*",
    "setbased/abc-html": "^1.0.0 || ^2.0.0",
    "setbased/exception": "^1.1.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0",
    "setbased/php-stratum": "^4.0.0",
    "setbased/php-stratum-mysql": "^4.0.0"
  },
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\Babel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Babel\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/"
  }
}
