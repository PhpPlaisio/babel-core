{
  "name": "plaisio/babel-core",
  "description": "PhpPlaisio: Core Babel",
  "keywords": [
    "PhpPlaisio",
    "Babel",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "plaisio/babel": "^1.0.0",
    "plaisio/html": "^3.0.0",
    "plaisio/kernel": "^1.0.0.",
    "setbased/exception": "^2.0.0"
  },
  "require-dev": {
    "ext-mysqli": "*",
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0",
    "setbased/php-stratum": "^5.0.0",
    "setbased/php-stratum-mysql": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Babel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Babel\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/"
  }
}
